{
  "name": "react-ts-template",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "ci": "pnpm run ci:lint && pnpm run ci:test",
    "ci:lint": "pnpm lint",
    "ci:test": "pnpm test",
    "dev": "vite",
    "format": "prettier --write .",
